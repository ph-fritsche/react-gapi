{
    "name": "react-gapi",
    "version": "0.0.1",
    "description": "Google API per React hook",
    "keywords": [
        "Google",
        "API",
        "googleapis"
    ],
    "repository": "https://github.com/ph-fritsche/react-gapi.git",
    "author": "Philipp Fritsche",
    "license": "MIT",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "node": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js",
            "default": "./dist/cjs/index.js"
        },
        "./testing": {
            "node": "./dist/cjs/testing.js",
            "import": "./dist/esm/testing.js",
            "default": "./dist/cjs/testing.js"
        }
    },
    "typesVersions": {
        "*": {
            "dist/types/index.d.ts": [
                "./dist/types/index.d.ts"
            ],
            "*": [
                "dist/types/*.d.ts"
            ]
        }
    },
    "files": [
        "dist/"
    ],
    "dependencies": {
        "react-weblibrary": "^1.0.1"
    },
    "peerDependencies": {
        "react": ">=16.8"
    },
    "devDependencies": {
        "@ph.fritsche/eslint-config": "^1.0.1",
        "@ph.fritsche/scripts-config": "^2.0.1",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/gapi": "^0.0.41",
        "@types/gapi.auth2": "^0.0.55",
        "@types/gapi.client": "^1.0.5",
        "@types/react": "^17.0.22",
        "eslint": "^7.32.0",
        "jest": "^27.2.0",
        "jest-tobetype": "^1.2.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "shared-scripts": "^1.4.1",
        "ts-jest": "^27.0.5",
        "typescript": "^4.4.3"
    },
    "scripts": {
        "build": "scripts ts-build",
        "lint": "eslint src test",
        "test": "jest"
    }
}
